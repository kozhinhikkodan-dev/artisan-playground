{
  "name": "kozhinhikkodan-dev/artisan-playground",
  "version": "1.0.3",
  "description": "A beautiful, secure, and feature-rich UI for executing Laravel Artisan commands with role-based access control, theme support, and comprehensive command management",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "artisan",
    "commands",
    "ui",
    "dashboard",
    "admin",
    "permissions",
    "bootstrap",
    "theme"
  ],
  "homepage": "https://github.com/kozhinhikkodan-dev/artisan-playground",
  "support": {
    "issues": "https://github.com/kozhinhikkodan-dev/artisan-playground/issues",
    "source": "https://github.com/kozhinhikkodan-dev/artisan-playground"
  },
  "authors": [
    {
      "name": "Salih Kozhinhikkodan",
      "email": "salih@kozhinhikkodan.dev",
      "homepage": "https://kozhinhikkodan.dev"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0",
    "spatie/laravel-permission": "^5.0|^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "KozhinhikkodanDev\\ArtisanPlayground\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KozhinhikkodanDev\\ArtisanPlayground\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "KozhinhikkodanDev\\ArtisanPlayground\\Providers\\ArtisanPlaygroundServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
